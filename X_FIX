function MC()
  KR = gg.choice({
    "🌸 Telegram  : @PUBGMSCRIPT",
    "🌸 Instagram : @PubgMobileScript",
	"🌸 Whatsapp  : +62 823 3213 9274",
	"🌸 Channel   : youtube.com/PUBGMCorner",
    "⏪⏪⏪"
  }, nil, "☎️ CONTACT US FOR VIP AND VVIP SCRIPT ☎️")
  if KR == 1 then
    MC()
  end
  if KR == 2 then
    MC()
  end
    if KR == 3 then
    MC()
  end
  if KR == 4 then
    MC()
  end
  if KR == 5 then
    HOME()
  end
  HOMEDM = -1
end
